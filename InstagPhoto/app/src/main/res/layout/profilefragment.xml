<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <android.support.design.widget.AppBarLayout
        android:id="@+id/appbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">

        <android.support.design.widget.CollapsingToolbarLayout
            android:id="@+id/collapsing_toolbar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            app:contentScrim="?attr/colorPrimary"
            app:layout_scrollFlags="scroll|exitUntilCollapsed">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">


                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/ivCoverPhoto"
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:background="@drawable/default_cover_photo"
                        android:scaleType="fitXY" />

                    <ProgressBar
                        android:id="@+id/pbCoverPhoto"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/editCoverPhoto"
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:layout_centerHorizontal="true"
                        android:layout_gravity="right"
                        android:layout_marginBottom="@dimen/_5dp"
                        android:layout_marginTop="125dp"
                        android:background="@drawable/edit_box_border"
                        android:drawablePadding="@dimen/_5dp"
                        android:drawableRight="@drawable/ic_camera_black"
                        android:drawableTint="@color/black"
                        android:gravity="center"
                        android:padding="@dimen/_5dp"
                        android:text="EDIT"
                        android:textColor="@color/black"
                        android:textSize="12sp" />


                    <FrameLayout
                        android:id="@+id/flProfile"
                        android:layout_width="@dimen/_100dp"
                        android:layout_height="@dimen/_100dp"
                        android:layout_marginLeft="@dimen/_35dp"
                        android:layout_marginStart="@dimen/_35dp"
                        android:layout_marginTop="100dp">

                        <ImageView
                            android:id="@+id/ivProfile1"
                            android:layout_width="@dimen/_100dp"
                            android:layout_height="@dimen/_100dp"
                            android:scaleType="centerCrop"
                            android:src="@drawable/default_profile_photo" />
                        <!-- app:sriv_left_bottom_corner_radius="48dip"
                         app:sriv_left_top_corner_radius="16dip"
                         app:sriv_oval="true"
                         app:sriv_right_bottom_corner_radius="16dip"
                         app:sriv_right_top_corner_radius="0dip"-->

                        <ProgressBar
                            android:id="@+id/pbProfilePhoto"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:visibility="gone" />


                        <TextView
                            android:id="@+id/editProfilePhoto"
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:layout_centerHorizontal="true"
                            android:layout_gravity="bottom|left"
                            android:background="@drawable/edit_box_border"
                            android:drawablePadding="@dimen/_5dp"
                            android:drawableRight="@drawable/ic_camera_black"
                            android:drawableTint="@color/black"
                            android:gravity="center"
                            android:paddingLeft="@dimen/_5dp"
                            android:text="EDIT"
                            android:textColor="@color/black"
                            android:textSize="10sp" />


                    </FrameLayout>

                    <TextView
                        android:id="@+id/txtUserName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:layout_marginLeft="150dp"
                        android:layout_marginStart="150dp"
                        android:gravity="bottom"
                        android:text="UserName"
                        android:textColor="@color/black"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                </FrameLayout>


                <RelativeLayout
                    android:id="@+id/topHeadview"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5dp">


                    <TableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:orientation="horizontal">

                        <TableRow
                            android:id="@+id/llpostscount"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/txtFriendsCount"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="0"
                                android:textColor="@color/black"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/txtPostsCount"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="0"
                                android:textColor="@color/black"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/txtFollowersCount"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="0"
                                android:textColor="@color/black"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/txtFolloweringCount"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="0"
                                android:textColor="@color/black"
                                android:textSize="16sp"
                                android:textStyle="bold" />
                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="friends"
                                android:textColor="@color/grey1"
                                android:textSize="12sp"
                                android:textStyle="normal" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="posts"
                                android:textColor="@color/grey1"
                                android:textSize="12sp"
                                android:textStyle="normal" />


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="@string/followers"
                                android:textColor="@color/grey1"
                                android:textSize="12sp"
                                android:textStyle="normal" />


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="@string/following"
                                android:textColor="@color/grey1"
                                android:textSize="12sp"
                                android:textStyle="normal" />
                        </TableRow>

                    </TableLayout>

                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="@dimen/_5dp"
                    android:background="@color/litgrey" />

            </LinearLayout>
        </android.support.design.widget.CollapsingToolbarLayout>

    </android.support.design.widget.AppBarLayout>

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:focusableInTouchMode="true"
            android:orientation="vertical">

            <TextView
                android:id="@+id/editProfileDetail"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_centerHorizontal="true"
                android:layout_gravity="right"
                android:layout_marginTop="@dimen/_2dp"
                android:background="@drawable/edit_box_border"
                android:drawablePadding="@dimen/_5dp"
                android:drawableRight="@drawable/ic_mode_edit_black_24dp"
                android:gravity="center"
                android:padding="@dimen/_5dp"
                android:text="Edit Details"
                android:textAllCaps="false"
                android:textColor="@color/black"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/txtBiography"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_5dp"
                android:layout_marginRight="@dimen/_5dp"
                android:background="@color/white"
                android:gravity="left"
                android:maxLength="150"
                android:paddingBottom="@dimen/_2dp"
                android:paddingLeft="@dimen/_5dp"
                android:paddingRight="@dimen/_5dp"
                android:text=""
                android:textColor="@color/black"
                android:textSize="14sp"
                android:textStyle="normal"
                android:visibility="visible" />

            <LinearLayout
                android:id="@+id/llStateCountry"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/white"
                android:focusableInTouchMode="true"
                android:orientation="horizontal"
                android:padding="@dimen/_3dp">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:background="@color/white"
                    android:gravity="left"
                    android:paddingLeft="@dimen/_5dp"
                    android:paddingTop="@dimen/_5dp"
                    android:text="City :"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    android:textStyle="normal"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/txtStateCity"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:background="@color/white"
                    android:gravity="left"
                    android:hint=" ---"
                    android:paddingLeft="@dimen/_5dp"
                    android:paddingTop="@dimen/_5dp"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    android:textStyle="normal"
                    android:visibility="visible" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:background="@color/white"
                    android:gravity="left"
                    android:paddingLeft="@dimen/_5dp"
                    android:paddingTop="@dimen/_5dp"
                    android:text=" Country :"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    android:textStyle="normal"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/txtCountryValue"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/_5dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:gravity="left"
                    android:hint=" ---"
                    android:textColor="@color/blackdark"
                    android:textSize="14sp"
                    android:textStyle="normal"
                    android:visibility="visible" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/llProfileContEmailID"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/white"
                android:focusableInTouchMode="true"
                android:orientation="horizontal"
                android:padding="@dimen/_3dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:background="@color/white"
                    android:gravity="left"
                    android:paddingLeft="@dimen/_5dp"
                    android:paddingTop="@dimen/_5dp"
                    android:text="Contact :"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    android:textStyle="normal"
                    android:visibility="visible" />


                <TextView
                    android:id="@+id/txtProfileContactNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:background="@color/white"
                    android:gravity="left"
                    android:hint=" ---"
                    android:paddingLeft="@dimen/_5dp"
                    android:paddingTop="@dimen/_5dp"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    android:textStyle="normal"
                    android:visibility="visible" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:background="@color/white"
                    android:gravity="left"
                    android:paddingLeft="@dimen/_5dp"
                    android:paddingTop="@dimen/_5dp"
                    android:text="Email :"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    android:textStyle="normal"
                    android:visibility="visible" />


                <TextView
                    android:id="@+id/txtProfileEmailId"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/_5dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:gravity="left"
                    android:hint=" ---"
                    android:textColor="@color/blackdark"
                    android:textSize="14sp"
                    android:textStyle="normal"
                    android:visibility="visible" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:padding="@dimen/_3dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:paddingLeft="@dimen/_5dp"
                    android:paddingTop="@dimen/_5dp"
                    android:text="Private Status :"
                    android:textColor="@color/black"
                    android:textSize="14sp" />


                <TextView
                    android:id="@+id/txtProfileStatus"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:background="@color/white"
                    android:gravity="left"
                    android:hint=" ---"
                    android:paddingLeft="@dimen/_5dp"
                    android:paddingTop="@dimen/_5dp"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    android:textStyle="normal"
                    android:visibility="visible" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:padding="@dimen/_3dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:paddingLeft="@dimen/_5dp"
                    android:paddingTop="@dimen/_5dp"
                    android:text="Web Info :"
                    android:textColor="@color/black"
                    android:textSize="14sp" />


                <TextView
                    android:id="@+id/txtProfileWebInfo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:background="@color/white"
                    android:gravity="left"
                    android:hint=" ---"
                    android:paddingLeft="@dimen/_5dp"
                    android:paddingTop="@dimen/_5dp"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    android:textStyle="normal"
                    android:visibility="visible" />
            </LinearLayout>


            <TextView
                android:id="@+id/txtProfileBusinessPage"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_5dp"
                android:background="@color/white"
                android:gravity="left"
                android:paddingLeft="@dimen/_8dp"
                android:paddingTop="@dimen/_3dp"
                android:text="Business Page"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:textStyle="normal" />

            <ListView
                android:id="@+id/listview_business"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/_3dp"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/btnBusinessPage"
                    android:layout_width="0dp"
                    android:layout_height="35dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:layout_weight="1"
                    android:background="@color/participant_view_selected"
                    android:gravity="center"
                    android:padding="@dimen/_8dp"
                    android:text="Business Page"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    android:textStyle="normal" />

                <Button
                    android:id="@+id/btnBusinessGroup"
                    android:layout_width="0dp"
                    android:layout_height="35dp"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:layout_weight="1"
                    android:background="@color/participant_view_selected"
                    android:gravity="center"
                    android:padding="@dimen/_8dp"
                    android:text="Groups"
                    android:textColor="@color/white"

                    android:textSize="14sp"
                    android:textStyle="normal" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5dp"
                android:background="@drawable/bottomtopline"
                android:orientation="horizontal"
                android:padding="@dimen/_5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_10dp"
                    android:padding="@dimen/_5dp"
                    android:text="Frineds"
                    android:textColor="@color/black"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/badge_friends"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:background="@drawable/round_count"
                    android:gravity="center"
                    android:text="1"
                    android:textColor="@color/white"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    android:visibility="gone" />


            </LinearLayout>


            <com.instag.vijay.fasttrending.view.ExpandableHeightGridView
                android:id="@+id/gv_friends_list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8dp"
                android:fadingEdge="none"
                android:horizontalSpacing="@dimen/_2dp"
                android:numColumns="4"
                android:stretchMode="columnWidth"
                android:verticalSpacing="@dimen/_10dp" />

            <TextView
                android:id="@+id/txt_see_all_friends"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/_15dp"
                android:gravity="center"
                android:text="See all friends >>"
                android:textColor="@color/grey"
                android:textSize="12sp"
                android:textStyle="bold"
                android:visibility="visible" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5dp"
                android:background="@drawable/bottomtopline"
                android:orientation="horizontal"
                android:padding="@dimen/_5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_10dp"
                    android:padding="@dimen/_5dp"
                    android:text="Following"
                    android:textColor="@color/black"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/badge_following"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:background="@drawable/round_count"
                    android:gravity="center"
                    android:text="1"
                    android:textColor="@color/white"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    android:visibility="gone" />


            </LinearLayout>


            <com.instag.vijay.fasttrending.view.ExpandableHeightGridView
                android:id="@+id/gv_following_list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8dp"
                android:fadingEdge="none"
                android:horizontalSpacing="@dimen/_2dp"
                android:numColumns="4"
                android:stretchMode="columnWidth"
                android:verticalSpacing="@dimen/_10dp" />

            <TextView
                android:id="@+id/txt_see_all_followings"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/_15dp"
                android:gravity="center"
                android:text="See all followings >>"
                android:textColor="@color/grey"
                android:textSize="12sp"
                android:textStyle="bold"
                android:visibility="visible" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5dp"
                android:background="@drawable/bottomtopline"
                android:orientation="horizontal"
                android:padding="@dimen/_5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_10dp"
                    android:padding="@dimen/_5dp"
                    android:text="Followers"
                    android:textColor="@color/black"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/badge_followers"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:background="@drawable/round_count"
                    android:gravity="center"
                    android:text="1"
                    android:textColor="@color/white"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    android:visibility="gone" />


            </LinearLayout>


            <com.instag.vijay.fasttrending.view.ExpandableHeightGridView
                android:id="@+id/gv_followers_list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8dp"
                android:fadingEdge="none"
                android:horizontalSpacing="@dimen/_2dp"
                android:numColumns="4"
                android:stretchMode="columnWidth"
                android:verticalSpacing="@dimen/_10dp" />

            <TextView
                android:id="@+id/txt_see_all_followers"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/_15dp"
                android:gravity="center"
                android:text="See all followers >>"
                android:textColor="@color/grey"
                android:textSize="12sp"
                android:textStyle="bold"
                android:visibility="visible" />

            <LinearLayout
                android:id="@+id/titlebar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="3dp"
                android:layout_marginLeft="7dp"
                android:layout_marginRight="7dp"
                android:layout_marginTop="3dp"
                android:orientation="horizontal"
                android:visibility="visible">


                <FrameLayout
                    android:id="@+id/viewYou"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/bottomlinecolor">

                    <TextView
                        android:id="@+id/txtYou"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_margin="3dp"
                        android:gravity="center"
                        android:padding="5dp"
                        android:text="You"
                        android:textColor="@color/black"
                        android:textSize="16sp" />

                </FrameLayout>


                <FrameLayout
                    android:id="@+id/viewFriends"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/txtFriends"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_margin="3dp"
                        android:gravity="center"
                        android:padding="5dp"
                        android:text="Friends"
                        android:textColor="@color/black"
                        android:textSize="16sp" />

                </FrameLayout>

                <FrameLayout
                    android:id="@+id/viewLikes"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/txtLikes"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_margin="3dp"
                        android:gravity="center"
                        android:padding="5dp"
